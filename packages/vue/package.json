{
  "name": "@sentry/vue",
  "version": "7.76.0",
  "description": "Official Sentry SDK for Vue.js",
  "repository": "git://github.com/getsentry/sentry-javascript.git",
  "homepage": "https://github.com/getsentry/sentry-javascript/tree/master/packages/vue",
  "author": "Sentry",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "build/npm/cjs/index.js",
  "module": "build/npm/esm/index.js",
  "types": "build/npm/types/index.d.ts",
  "typesVersions": {
    "<4.9": {
      "build/types/index.d.ts": [
        "build/types-ts3.8/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sentry/browser": "7.76.0",
    "@sentry/core": "7.76.0",
    "@sentry/types": "7.76.0",
    "@sentry/utils": "7.76.0"
  },
  "peerDependencies": {
    "vue": "2.x || 3.x"
  },
  "devDependencies": {
    "vue": "~3.2.41"
  },
  "scripts": {
    "build": "run-p build:transpile build:types",
    "build:bundle": "run-p build:bundle:es5 build:bundle:es6",
    "build:bundle:es5": "JS_VERSION=es5 rollup -c rollup.bundle.config.js",
    "build:bundle:es6": "JS_VERSION=es6 rollup -c rollup.bundle.config.js",
    "build:dev": "run-p build:transpile build:types",
    "build:transpile": "rollup -c rollup.npm.config.js",
    "build:types": "run-s build:types:core build:types:downlevel",
    "build:types:core": "tsc -p tsconfig.types.json",
    "build:types:downlevel": "yarn downlevel-dts build/types build/types-ts3.8 --to ts3.8",
    "build:watch": "run-p build:transpile:watch build:types:watch",
    "build:dev:watch": "run-p build:transpile:watch build:types:watch",
    "build:transpile:watch": "rollup -c rollup.npm.config.js --watch",
    "build:types:watch": "tsc -p tsconfig.types.json --watch",
    "build:tarball": "ts-node ../../scripts/prepack.ts && npm pack ./build/npm",
    "circularDepCheck": "madge --circular src/index.ts",
    "clean": "rimraf build coverage sentry-vue-*.tgz",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint . --format stylish --fix",
    "fix:prettier": "prettier --write \"{src,test,scripts}/**/**.ts\"",
    "lint": "run-s lint:prettier lint:eslint",
    "lint:eslint": "eslint . --format stylish",
    "lint:prettier": "prettier --check \"{src,test,scripts}/**/**.ts\"",
    "validate:es5": "es-check es5 'build/bundles/*.es5*.js'",
    "size:check": "npm run size:check:es5 && npm run size:check:es6 && npm run size:check:tracing:es5 && npm run size:check:tracing:es6 && npm run size:check:replay:es6 && npm run size:check:tracing.replay:es6",
    "size:check:es5": "cat build/bundles/sentry-vue.es5.min.js | gzip -9 | wc -c | awk '{$1=$1/1024; print \"ES5:          \",$1,\"kB\";}'",
    "size:check:es6": "cat build/bundles/sentry-vue.min.js | gzip -9 | wc -c | awk '{$1=$1/1024; print \"ES6:          \",$1,\"kB\";}'",
    "size:check:tracing:es5": "cat build/bundles/sentry-vue.tracing.es5.min.js | gzip -9 | wc -c | awk '{$1=$1/1024; print \"ES5(tracing): \",$1,\"kB\";}'",
    "size:check:tracing:es6": "cat build/bundles/sentry-vue.tracing.min.js | gzip -9 | wc -c | awk '{$1=$1/1024; print \"ES6(tracing): \",$1,\"kB\";}'",
    "size:check:replay:es6": "cat build/bundles/sentry-vue.replay.min.js | gzip -9 | wc -c | awk '{$1=$1/1024; print \"ES6(replay): \",$1,\"kB\";}'",
    "size:check:tracing.replay:es6": "cat build/bundles/sentry-vue.tracing.replay.min.js | gzip -9 | wc -c | awk '{$1=$1/1024; print \"ES6(tracing.replay): \",$1,\"kB\";}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "yalc:publish": "ts-node ../../scripts/prepack.ts && yalc publish build --push --sig"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "sideEffects": false
}
